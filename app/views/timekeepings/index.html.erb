<p id="notice"><%= notice %></p>

<h1>Listing Timekeepings</h1>
<table>
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Clock in</th>
      <th>Clock out</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timekeepings.each do |timekeeping| %>
      <tr>
        <td><%= timekeeping.user.username %></td>
        <td><%= timekeeping.clock_in %></td>
        <td><%= timekeeping.clock_out %></td>
        <td><%= link_to 'Show', timekeeping %></td>
        <td><%= link_to 'Edit', edit_timekeeping_path(timekeeping) %></td>
        <td><%= link_to 'Destroy', timekeeping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<%= link_to 'New Timekeeping', new_timekeeping_path %>
